<wadl:application xmlns:wadl="http://wadl.dev.java.net/2009/02" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <wadl:doc xmlns:enunciate="http://enunciate.codehaus.org/" enunciate:generatedBy="Enunciate-1.25"/>
  <wadl:grammars>
    <wadl:include href="divelog.xsd"/>
    <wadl:include href="dl-security.xsd"/>
  </wadl:grammars>
  <wadl:resources base="http://localhost:8080/novaglobalapi">
    <wadl:resource path="/rest/user">
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[Gets all Users.

<ul>
<li>Status 200: Users Found.</li>
</ul>]]>
        </wadl:doc>
        <wadl:response>
          <wadl:doc>
            <![CDATA[List of Users]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/rest/security/identify">
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[Identifies the currently logged in REST user.

<ul>
<li>Status 200: Request Successful.</li>
</ul>]]>
        </wadl:doc>
        <wadl:response>
          <wadl:doc>
            <![CDATA[User Account]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/rest/security/login">
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Identifies the currently logged in REST user.

<ul>
<li>Status 202: Login Successful.</li>
<li>Status 401: Login Failed.</li>
</ul>]]>
        </wadl:doc>
        <wadl:request>
          <wadl:doc>
            <![CDATA[Users Login Credentials]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[User API Token]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/rest/security/logout">
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Logout the Current REST User.

<ul>
<li>Status 200: Logout Request Successful.</li>
</ul>]]>
        </wadl:doc>
        <wadl:response>
          <wadl:doc>
            <![CDATA[Logout Result]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/rest/user/find">
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Gets the User Specified by the Given Criteria. If not Criteria Specified, All Results will be returned.

<ul>
<li>Status 200: Results Found.</li>
<li>Status 400: Results not found.</li>
</ul>]]>
        </wadl:doc>
        <wadl:request>
          <wadl:doc>
            <![CDATA[Criteria to search for]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[Found users]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/rest/user/register">
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Registers a User.

<ul>
<li>Status 200: Successful Registration.</li>
<li>Status 400: Error with the Request. Most likely caused by an invalid UserAccount.</li>
</ul>]]>
        </wadl:doc>
        <wadl:request>
          <wadl:doc>
            <![CDATA[User to Register]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[Registered User]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/rest/user/{userName}">
      <wadl:param name="userName" style="template">
        <wadl:doc>
          <![CDATA[Username to serach for]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[Gets the User Specified by the Given Username.

<ul>
<li>Status 200: User Found.</li>
<li>Status 400: User was not found.</li>
</ul>]]>
        </wadl:doc>
        <wadl:request/>
        <wadl:response>
          <wadl:doc>
            <![CDATA[Found user]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="PUT">
        <wadl:doc>
          <![CDATA[Updates the User Specified by the Given Username.

<ul>
<li>Status 200: User Updated.</li>
</ul>]]>
        </wadl:doc>
        <wadl:request>
          <wadl:doc>
            <![CDATA[Updated User Account]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[Updated user]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="DELETE">
        <wadl:doc>
          <![CDATA[Deletes the User Specified by the Given Username.

<ul>
<li>Status 200: User Delete.</li>
</ul>]]>
        </wadl:doc>
        <wadl:request/>
        <wadl:response>
          <wadl:doc>
            <![CDATA[Delete status]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
  </wadl:resources>
</wadl:application>
